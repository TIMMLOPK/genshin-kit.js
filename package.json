{
  "name": "genshin-kit.js",
  "version": "0.0.0",
  "description": "Hoyolab API unofficial library",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:apps": "turbo run build:local --filter='...{apps/*}'",
    "test": "turbo run test --parallel",
    "lint": "turbo run lint --parallel",
    "format": "turbo run format --parallel",
    "docs": "turbo run docs --parallel"
  },
  "repository": {
    "url": "https://github.com/TIMMLOPK/genshin-kit.js"
  },
  "homepage": "https://github.com/TIMMLOPK/genshin-kit.js",
  "author": "TIMMLOPK",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}