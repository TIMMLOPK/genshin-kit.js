    "@types/md5": "^2.3.2",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "jest": "^29.5.0",
