{
  "name": "genshin-kit.js",
  "version": "2.4.11",
  "description": "Hoyolab API unofficial library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "genshin",
    "HoYoLAB",
    "mihoyo"
  ],
  "scripts": {
    "build:check": "tsc --noEmit",
    "build:clean": "del-cli ./dist",
    "build:compile": "tsc",
    "build": "yarn build:check && yarn build:clean && yarn build:compile",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest --coverage"
  },
  "repository": {
    "url": "https://github.com/TIMMLOPK/genshin-kit.js"
  },
  "homepage": "https://github.com/TIMMLOPK/genshin-kit.js",
  "author": "TIMMLOPK",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.6.2",
    "undici": "^5.27.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "del-cli": "^5.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}