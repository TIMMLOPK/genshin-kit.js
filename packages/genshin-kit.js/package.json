{
  "name": "genshin-kit.js",
  "version": "2.4.13",
  "description": "Hoyolab API unofficial library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "genshin",
    "HoYoLAB",
    "mihoyo"
  ],
  "scripts": {
    "build:check": "tsc --noEmit",
    "build:clean": "del-cli ./dist",
    "build:compile": "tsc",
    "build": "pnpm run build:check && pnpm run build:clean && pnpm run build:compile",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest --coverage"
  },
  "repository": {
    "url": "https://github.com/TIMMLOPK/genshin-kit.js"
  },
  "homepage": "https://github.com/TIMMLOPK/genshin-kit.js",
  "author": "TIMMLOPK",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.8.1",
    "undici": "^7.5.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "del-cli": "^6.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
