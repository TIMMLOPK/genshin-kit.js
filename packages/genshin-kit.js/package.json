{
  "name": "genshin-kit.js",
  "version": "2.4.8",
  "description": "Hoyolab API unofficial library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:check": "tsc --noEmit",
