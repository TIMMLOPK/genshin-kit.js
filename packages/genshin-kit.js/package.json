{
  "name": "genshin-kit.js",
  "version": "2.4.1",
  "description": "Hoyolab API unofficial library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:check": "tsc --noEmit",
    "build:clean": "del-cli ../../dist --force",
    "build:compile": "tsc",
    "build": "npm run build:check && npm run build:clean && npm run build:compile",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest --coverage"
  },
  "repository": {
    "url": "https://github.com/TIMMLOPK/genshin-kit.js"
  },
  "homepage": "https://github.com/TIMMLOPK/genshin-kit.js",
  "author": "TIMMLOPK",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.5.0",
    "turbo": "^1.9.3",
    "undici": "^5.21.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.9.0"
  }
