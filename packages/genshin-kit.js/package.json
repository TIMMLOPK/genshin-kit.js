  "license": "MIT",
  "dependencies": {
    "tslib": "^2.6.0",
    "undici": "^5.22.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/md5": "^2.3.2",
    "@types/node": "20.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "del-cli": "^5.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.40.0",
    "jest": "^29.6.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.9.0"
