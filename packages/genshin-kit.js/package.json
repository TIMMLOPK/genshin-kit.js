{
  "name": "genshin-kit.js",
  "version": "2.4.13",
  "description": "Hoyolab API unofficial library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "genshin",
    "HoYoLAB",
    "mihoyo"
  ],
  "scripts": {
    "build:check": "tsc --noEmit",
    "build:clean": "del-cli ./dist",
    "build:compile": "tsc",
    "build": "yarn build:check && yarn build:clean && yarn build:compile",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest --coverage"
  },
  "repository": {
    "url": "https://github.com/TIMMLOPK/genshin-kit.js"
  },
  "homepage": "https://github.com/TIMMLOPK/genshin-kit.js",
  "author": "TIMMLOPK",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.6.3",
    "undici": "^6.18.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "22.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "del-cli": "^5.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.4",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
